{{2,4,0},{3,1,-10},{4,1,0},{5,9,-10},{6,4,0},{7,2,0},{7,3,-10},{8,1,0},{9,1,0},{10,6,-10},{10,5,0},{11,7,-10},{12,7,0},{12,3,0},{13,11,0},{14,12,0},{15,12,0}},
